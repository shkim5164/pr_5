<p id="notice"><%= notice %></p>

<h1>Listing Posts</h1>

<table>
  <thead>
    <tr>
      <th>Buyer</th>
      <th>St time</th>
      <th>End time</th>
      <th>Runningtime</th>
      <th>St price</th>
      <th>End price</th>
      <th>Bn price</th>
      <th>Up price</th>
      <th>Au type</th>
      <th>Category</th>
      <th>Like</th>
      <th>Img address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.buyer_id %></td>
        <td><%= post.st_time %></td>
        <td><%= post.end_time %></td>
        <td><%= post.runningtime %></td>
        <td><%= post.st_price %></td>
        <td><%= post.end_price %></td>
        <td><%= post.bn_price %></td>
        <td><%= post.up_price %></td>
        <td><%= post.au_type %></td>
        <td><%= post.category %></td>
        <td><%= post.like %></td>
        <td><%= post.img_address %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
